{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "description": "Schema for attribute_sets data type - array of attribute set definitions",
  "items": {
    "type": "object",
    "properties": {
      "attributeSet": {
        "type": "object",
        "properties": {
          "attribute_set_name": { "type": "string" },
          "sort_order": { "type": "number" },
          "entity_type_id": { "type": "number" }
        },
        "required": ["attribute_set_name", "entity_type_id"]
      },
      "skeletonId": { "type": "number" }
    },
    "required": ["attributeSet"]
  }
}

